{
  "mcpServers": {
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "disabled": false,
      "autoApprove": ["sequentialthinking"]
    },
    "neon": {
      "command": "bunx",
      "args": ["mcp-remote", "https://mcp.neon.tech/mcp", "3334"],
      "disabled": false,
      "autoApprove": [
        "list_projects",
        "get_project",
        "get_database_tables",
        "describe_table_schema",
        "get_database_schema",
        "list_databases"
      ]
    },
    "cloudflare-bindings": {
      "command": "bunx",
      "args": ["mcp-remote", "https://bindings.mcp.cloudflare.com/mcp", "3335"],
      "disabled": false,
      "autoApprove": [
        "accounts_list",
        "workers_list",
        "workers_get_worker",
        "workers_get_worker_code",
        "kv_namespaces_list",
        "kv_namespace_get",
        "r2_buckets_list",
        "r2_bucket_get",
        "d1_databases_list",
        "d1_database_get",
        "hyperdrive_configs_list",
        "hyperdrive_config_get"
      ]
    },
    "cloudflare-builds": {
      "command": "bunx",
      "args": ["mcp-remote", "https://builds.mcp.cloudflare.com/mcp", "3336"],
      "disabled": true,
      "autoApprove": [
        "workers_builds_list_builds",
        "workers_builds_get_build",
        "workers_builds_get_build_logs"
      ]
    },
    "cloudflare-observability": {
      "command": "bunx",
      "args": ["mcp-remote", "https://observability.mcp.cloudflare.com/mcp", "3337"],
      "disabled": true,
      "autoApprove": [
        "observability_keys",
        "observability_values",
        "query_worker_observability"
      ]
    },
    "cloudflare-docs": {
      "command": "bunx",
      "args": ["mcp-remote", "https://docs.mcp.cloudflare.com/mcp", "3338"],
      "disabled": true,
      "autoApprove": ["search_cloudflare_documentation"]
    }
  }
}
