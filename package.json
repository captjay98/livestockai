{
  "name": "livestock-ai",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite dev --port 3001",
    "build": "vite build",
    "start": "node .output/server/index.mjs",
    "preview": "wrangler dev",
    "test": "vitest run",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:all": "vitest run && vitest run --config vitest.integration.config.ts",
    "test:coverage": "vitest run --coverage --config vitest.coverage.config.ts",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "db:migrate": "bun run app/lib/db/migrate.ts",
    "db:rollback": "bun run app/lib/db/migrate.ts rollback",
    "db:reset": "bun run app/lib/db/reset.ts",
    "db:test:setup": "bun run scripts/setup-test-db.ts",
    "db:seed": "bun run app/lib/db/seeds/production.ts",
    "db:seed:prod": "bun run app/lib/db/seeds/production.ts",
    "db:seed:dev": "bun run app/lib/db/seeds/development.ts",
    "db:seed:demo": "bun run app/lib/db/seeds/seed-demo-for-existing-user.ts",
    "deploy": "vite build && wrangler deploy",
    "docs:generate": "typedoc"
  },
